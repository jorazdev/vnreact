    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.5.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
